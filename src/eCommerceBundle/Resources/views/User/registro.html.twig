{% extends 'base.html.twig' %}

{% block title %}Log in
{% endblock %}
{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('css/registro.css') }}"/>
{% endblock %}

{% block body %}

    <div class="container productos">
        <div class="row">
            <div class="card color">
                <div class="card-content">
                    <span class="card-title">Registro</span>
                    {{ form_start(form) }}
                    {{ form_widget(form) }}
                    <button class="btn color" type="submit">Registrar</button>
                    {{ form_end(form) }}
                </div>
            </div>
        </div>
    </div>

{% endblock %}
