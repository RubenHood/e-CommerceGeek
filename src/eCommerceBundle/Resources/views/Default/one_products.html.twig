{% extends "base.html.twig" %} {% block title %}Bienvenido a Geek {% endblock %} {% block stylesheets %}
<link rel="stylesheet" href="{{ asset('css/all_products.css') }}" /> {% endblock %} {% block body %}
<div class="row">
    <div class="col s4">
        <div class="card">
            <div class="card-image">
                <img src="../img/{{product.img}}">
                <a class="btn-floating halfway-fab waves-effect waves-light red">
                    <i class="material-icons">search</i>
                </a>
            </div>
            <div class="card-content">
                <span class="card-title nombre">{{product.name}}</span>
                <p class="precio">{{product.price}}$</p>
                <p class="recibelo">Rec√≠belo ya</p>
                <img src="{{ asset('img/stars.png') }}" class="imagen">
            </div>
        </div>
    </div>
</div>
{% endblock %} {% block javascripts %}{% endblock %}